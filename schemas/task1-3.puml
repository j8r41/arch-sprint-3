@startuml ER-Diagram

entity House {
    * id
    --
    * user_id
    --
    * address
}

entity User {
    * id
    --
    * name
    --
    * surname
}

entity Module {
    * id
    --
    * house_id
    --
    * device_type_id
    --
    * serial_number
    ---
    * title
}

entity DeviceType {
    * id
    --
    * code
    --
    * description
}

entity Device {
    * id
    --
    * module_id
    --
    * serial_number
    --
    * status
}

entity Telemetry {
    * id
    --
    * device_id
    --
    * datetime
    --
    * data
}

User ---{ House
House ---{ Module
Module ---{ DeviceType
Module ---{  Device
Device ---{ Telemetry
@enduml
